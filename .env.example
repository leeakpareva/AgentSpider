# Copy this file to .env.local for local development
# Or set these in Vercel's environment variables for production

# OpenAI API Key for chat functionality
OPENAI_API_KEY=your_openai_api_key_here

# OpenAI Assistant ID (optional)
ASSISTANT_ID=your_assistant_id_here

# Voice Prompt ID (optional)
VOICE_PROMPT=your_voice_prompt_id_here

# Vercel API Key (optional)
VERCEL_API_KEY=your_vercel_api_key_here

# Vercel Project ID
VERCEL_PROJECT_ID=your_vercel_project_id_here

# API URL for local development (optional)
# Defaults to http://192.168.0.32:3001/api if not set
VITE_API_URL=http://192.168.0.32:3001/api